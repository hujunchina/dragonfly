    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "merge-json": "^0.1.0-b.3",
    "moment": "^2.27.0",
    "path-to-regexp": "^6.1.0",
    "sass-loader": "^9.0.2",
    "store": "^2.0.12",
    "underscore": "^1.10.2",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1",
    "xlsx": "^0.16.6"
  },
